<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <rotate
            android:fromDegrees="175"
            android:toDegrees="135"
            android:pivotX="50%"
            android:pivotY="50%" >
            <shape
                android:shape="line" >
                <stroke
                    android:width="3dip"
                    android:color="#581EC3"
                    android:dashWidth="60px"
                    android:dashGap="40px"
                    />
            </shape>
        </rotate>
    </item>
</layer-list>